{
  "name": "famshare",
  "description": "Easily set up recurring payments based on plans.",
  "version": "0.0.1",
  "author": "BeeeQueue",
  "dependencies": {
    "body-parser": "1.18.3",
    "boom": "7.3.0",
    "cookie-parser": "1.4.3",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "express-graphql": "0.7.1",
    "express-winston": "3.1.0",
    "graphql": "14.1.1",
    "js-base64": "2.5.0",
    "knex": "0.16.3",
    "pg": "7.7.1",
    "rambdax": "1.3.0",
    "stripe": "6.20.0",
    "superagent": "4.0.0",
    "ts-node": "7.0.1",
    "tsconfig-paths": "3.7.0",
    "typescript": "3.3.3333",
    "uuid": "3.3.2",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/boom": "7.2.1",
    "@types/cookie-parser": "1.4.1",
    "@types/express": "4.16.0",
    "@types/express-graphql": "0.6.2",
    "@types/express-winston": "3.0.1",
    "@types/graphql": "14.0.7",
    "@types/js-base64": "2.3.1",
    "@types/knex": "0.15.1",
    "@types/stripe": "6.18.0",
    "@types/superagent": "3.8.5",
    "@types/uuid": "3.4.4",
    "graphql-code-generator": "0.18.0",
    "graphql-codegen-typescript-common": "0.18.0",
    "graphql-codegen-typescript-server": "0.18.0",
    "husky": "1.3.0",
    "lint-staged": "8.1.0",
    "node-sass": "4.11.0",
    "prettier": "1.15.3",
    "sass-loader": "7.1.0",
    "ts-node-dev": "1.0.0-pre.32"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "AGPL-3.0-only",
  "lint-staged": {
    "*.{js,ts,json,vue}": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn -r dotenv/config -r tsconfig-paths/register --inspect -- src/index.ts",
    "start": "ts-node -r dotenv/config -r tsconfig-paths/register src/index.ts",
    "proxy": "ngrok http -subdomain famshare 3000"
  }
