  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "AGPL-3.0-only",
  "lint-staged": {
    "*.{js,ts,json,vue}": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "scripts": {
    "build": "yarn tsc --noEmit -P tsconfig.json",
    "dev": "ts-node-dev --respawn -r dotenv/config -r tsconfig-paths/register --inspect -- src/index.ts",
    "lint": "eslint 'src/**/*'",
    "migrate": "knex migrate:latest --knexfile knexfile.ts",
    "migrate:create": "knex migrate:make --knexfile knexfile.ts -x ts",
    "prestart": "yarn run migrate",
    "pretest": "NODE_ENV=test yarn run migrate",
    "proxy": "ngrok start famshare famshare-ui",
    "start": "ts-node -r dotenv/config -r tsconfig-paths/register src/index.ts"
  }
}
